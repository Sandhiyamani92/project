<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Hollard Automation Testing on Windows Controller" parallel="tests" thread-count="15" verbose="5">
  <listeners>
        <listener class-name="com.sts.testautomation.listeners.TestListener"/>
        <listener class-name="com.sts.testautomation.listeners.AnnotationTransformer"/>
    </listeners>
  
  
   <test name="Test on Chrome" preserve-order="false">
  <parameter name="Device" value="Chrome"/>
       <parameter name="URL" value="https://core.nimbis.co.za"/>

  <classes>
    <class name="com.sts.testautomation.steps.Contents"/>
    </classes> 
  </test>

 
</suite>   